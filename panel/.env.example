# Panel listen
ELEGANTMC_PANEL_HOST=0.0.0.0
ELEGANTMC_PANEL_PORT=3000

# Admin password for Panel UI (cookie login). If empty, Panel will generate one and print it in logs.
ELEGANTMC_PANEL_ADMIN_PASSWORD=change-me
# Set to 1 only when Panel is served over HTTPS (Secure cookies)
# ELEGANTMC_PANEL_SECURE_COOKIE=1

# REQUIRED: daemon_id -> token mapping (JSON object)
# Example:
# {"my-node":"devtoken123"}
ELEGANTMC_DAEMON_TOKENS_JSON={"my-node":"devtoken123"}
